{
    "name": "mujhtech/laravel-varspay",
    "description": "PHP API Library for varspay service",
    "keywords": ["laravel", "varspay", "php", "payment", "api", "library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mujhtech",
            "email": "mujeeb.muhideen@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": ">=6.0",
        "php": ">=7.2.0",
        "illuminate/support": "~6|~7|~8"
    },
    "autoload": {
        "files": [
            "src/Helpers.php"
          ],
        "psr-4": {
            "Mujhtech\\Varspay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mujhtech\\Varspay\\VarspayServiceProvider"
            ],
            "aliases": {
                "Varspay": "Mujhtech\\Varspay\\Facades\\Varspay"
            }
        }
    }
}
